{
  "name": "@flex-development/errnode",
  "description": "Universal API for creating Node.js errors",
  "version": "1.2.0",
  "keywords": [
    "error",
    "node",
    "nodejs",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/flex-development/errnode",
  "repository": "https://github.com/flex-development/errnode.git",
  "bugs": "https://github.com/flex-development/errnode/issues",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "mkbuild",
    "check:ci": "yarn dedupe --check && yarn check:format && yarn check:lint && yarn check:spelling && yarn typecheck && yarn check:types:build && yarn test:cov && NODE_ENV=production yarn pack -o %s-%v.tgz && yarn clean:pack",
    "check:format": "prettier --check .",
    "check:lint": "eslint --exit-on-fatal-error --ext cjs,cts,gql,json,jsonc,md,mjs,ts,yml --max-warnings 0 .",
    "check:spelling": "cspell lint --color --no-progress --relative $@ \"**\"",
    "check:types": "tsc -p tsconfig.typecheck.json",
    "check:types:build": "tsc -p tsconfig.build.json",
    "check:upgrades": "yarn upgrade-interactive",
    "clean:build": "trash ./{dist,*.tgz}",
    "clean:coverage": "trash ./coverage",
    "clean:modules": "trash ./.yarn/{cache,*.gz} ./node_modules",
    "clean:pack": "trash ./*.tgz",
    "clean:test": "yarn clean:coverage && trash ./__tests__/report.json",
    "codecov": "yarn test:cov && yarn test:cov:upload",
    "codecov:validate": "cat .codecov.yml | curl --data-binary @- https://codecov.io/validate",
    "conventional-changelog": "node --loader=./loader.mjs ./node_modules/.bin/conventional-changelog -n ./changelog.config.cts",
    "fix:cg": "yarn fix:format && yarn fix:lint",
    "fix:dedupe": "yarn dedupe --strategy=highest",
    "fix:format": "prettier --cache --write .",
    "fix:lint": "yarn check:lint --cache --fix",
    "postinstall": "[ -f ./node_modules/.bin/husky ] && chmod +x .husky/* && husky install || exit 0",
    "postpack": "toggle-scripts +postinstall",
    "postpublish": "toggle-scripts +prepack",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "prepublishOnly": "toggle-scripts -prepack",
    "recommended-bump": "conventional-recommended-bump --preset=conventionalcommits --tag-prefix=$(jq .tagPrefix package.json -r) --verbose",
    "release": "bash ./scripts/release.sh",
    "test": "vitest run",
    "test:cov": "yarn clean:coverage; yarn test --coverage",
    "test:cov:serve": "serve -l 5432 ./coverage/lcov-report",
    "test:cov:upload": "./codecov -t $CODECOV_TOKEN -f ./coverage/lcov.info",
    "test:watch": "vitest",
    "typecheck": "vitest typecheck --run",
    "typecheck:watch": "vitest typecheck"
  },
  "dependencies": {
    "@flex-development/tutils": "6.0.0-alpha.8",
    "node-inspect-extracted": "2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@commitlint/types": "17.0.0",
    "@faker-js/faker": "7.6.0",
    "@flex-development/mkbuild": "1.0.0-alpha.9",
    "@flex-development/pkg-types": "1.0.0",
    "@graphql-eslint/eslint-plugin": "3.13.0",
    "@types/chai": "4.3.4",
    "@types/chai-string": "1.4.2",
    "@types/conventional-changelog": "3.1.1",
    "@types/conventional-changelog-config-spec": "2.1.2",
    "@types/conventional-changelog-writer": "4.0.2",
    "@types/conventional-commits-parser": "3.0.3",
    "@types/dateformat": "5.0.0",
    "@types/eslint": "8.4.10",
    "@types/git-raw-commits": "2.0.1",
    "@types/is-ci": "3.0.0",
    "@types/node": "16.18.11",
    "@types/node-notifier": "8.0.2",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vates/toggle-scripts": "1.0.0",
    "@vitest/coverage-c8": "0.26.2",
    "@vitest/ui": "0.26.2",
    "chai": "4.3.7",
    "chai-string": "1.5.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-recommended-bump": "6.1.0",
    "cspell": "6.18.1",
    "esbuild": "0.16.12",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-jsonc": "2.5.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-markdownlint": "0.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-yml": "1.4.0",
    "graphql": "16.6.0",
    "graphql-config": "4.4.0",
    "growl": "1.10.5",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.1.0",
    "node-notifier": "10.0.1",
    "prettier": "2.8.1",
    "prettier-plugin-sh": "0.12.8",
    "pretty-format": "29.3.1",
    "pupa": "3.1.0",
    "serve": "14.1.2",
    "sitemap": "7.1.1",
    "trash-cli": "5.0.0",
    "ts-dedent": "2.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "version-bump-prompt": "6.1.0",
    "vite": "4.0.3",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.2",
    "vitest-github-actions-reporter": "0.9.0",
    "yaml-eslint-parser": "1.1.0"
  },
  "bundleDependencies": [
    "node-inspect-extracted"
  ],
  "peerDependencies": {
    "@types/node": ">=14"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "resolutions": {
    "@ardatan/sync-fetch": "larsgw/sync-fetch#head=worker_threads",
    "@flex-development/tutils": "6.0.0-alpha.8",
    "esbuild": "0.16.12",
    "vite": "4.0.3",
    "vitest@npm:0.26.2": "patch:vitest@npm%3A0.26.2#patches/vitest+0.26.2.dev.patch"
  },
  "engines": {
    "node": ">=14.16",
    "yarn": "4.0.0-rc.34"
  },
  "packageManager": "yarn@4.0.0-rc.34",
  "readme": "README.md",
  "sideEffects": false,
  "tagPrefix": ""
}
